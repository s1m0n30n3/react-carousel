{
  "name": "react-image-carousel",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/react-image-carousel.umd.js",
  "module": "./dist/react-image-carousel.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-image-carousel.es.js",
      "require": "./dist/react-image-carousel.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage",
    "cy": "./node_modules/.bin/cypress open"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.3",
    "c8": "^7.12.0",
    "cypress": "^12.5.1",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.0",
    "react-test-renderer": "^18.2.0",
    "vite": "^4.0.0",
    "vitest": "^0.28.3"
  }
}
